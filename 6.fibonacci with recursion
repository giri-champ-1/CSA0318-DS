#include<stdio.h>
void printFibonacci(int n){
	static int n1=0,n2=1,n3;
	if(n>0){
		n3=n1+n2;
		n1=n2;
		n2=n3;
		printf("%d",n3);
		printFibonacci(n-1);}
	}
	int main(){
		int n;
		printf("Enter no of elements:");
		scanf("%d",&n);
		printf("Fibonacci series:");
		printf("%d%d",0,1);
		printFibonacci(n-2);
	return 0;
}

OUTPUT:


Enter no of elements:6
Fibonacci series:011235
